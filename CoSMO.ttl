###  http://purl.obolibrary.org/obo/IAO_0000136
<http://purl.obolibrary.org/obo/IAO_0000136> rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#actsAsContactPointFor
cosmo:actsAsContactPointFor rdf:type owl:ObjectProperty ,
                                     owl:AsymmetricProperty ,
                                     owl:IrreflexiveProperty ;
                            rdfs:domain cosmo:DataProtectionOfficer ;
                            rdfs:range [ rdf:type owl:Class ;
                                         owl:unionOf ( cosmo:Controller
                                                       cosmo:DataSubject
                                                       cosmo:NationalAuthority
                                                     )
                                       ] ;
                            <http://purl.org/dc/elements/1.1/identifier> "154-cosmo" ;
                            rdfs:comment "Indicates that the Data Protection Officer acts as a point of contact for other agents."@en ;
                            rdfs:isDefinedBy "BRASIL. Lei nº 13.709, de 14 de agosto de 2018. Lei Geral de Proteção de Dados Pessoais (LGPD). Brasília, 2018. Available at: https://www.in.gov.br/en/web/dou/-/lei-n-13.709-de-14-de-agosto-de-2018-174008209."@pt-br ;
                            rdfs:label "acts as contact point for"@en ,
                                       "canal de comunicação entre"@pt-br ;
                            skos:example "The Data Protection Officer acts as contact point for the Data Subject and the Controller."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#allowsManagementBy
cosmo:allowsManagementBy rdf:type owl:ObjectProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain cosmo:ConsentManagement ;
                         rdfs:range cosmo:DigitalSocialMediaUser ;
                         <http://purl.org/dc/elements/1.1/identifier> "155-cosmo" ;
                         rdfs:comment "Indicates that a consent management system allows management by a user."@en ;
                         rdfs:label "allows management by"@en ,
                                    "permite gerenciamento por"@pt-br ;
                         skos:example "A privacy dashboard allows management by the user."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#attemptToInfluence
cosmo:attemptToInfluence rdf:type owl:ObjectProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( cosmo:DarkPatternAction
                                                     cosmo:DarkPatternArtifact
                                                   )
                                     ] ;
                         rdfs:range cosmo:DigitalSocialMediaUser ;
                         <http://purl.org/dc/elements/1.1/identifier> "156-cosmo" ;
                         rdfs:comment "Indicates that a deceptive interface attempts to influence the user's decision."@en ;
                         rdfs:label "attempt to influence"@en ,
                                    "tenta influenciar"@pt-br ;
                         skos:example "A deceptive interface attempts to influence the user's decision."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#canLeadTo
cosmo:canLeadTo rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( cosmo:CognitiveBias
                                            cosmo:LimitationOfCognitiveCapacity
                                          )
                            ] ;
                rdfs:range cosmo:VitiatedConsent ;
                <http://purl.org/dc/elements/1.1/identifier> "157-cosmo" ;
                rdfs:comment "Indicates that a cognitive bias or limitation can be a contributing factor leading to a vitiated consent."@en ;
                rdfs:label "can lead to"@en ,
                           "pode levar a"@pt-br ;
                skos:example "A cognitive bias can lead to a vitiated consent."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#decidesOn
cosmo:decidesOn rdf:type owl:ObjectProperty ;
                owl:inverseOf cosmo:isDecidedBy ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain cosmo:Controller ;
                rdfs:range cosmo:Processing ;
                <http://purl.org/dc/elements/1.1/identifier> "158-cosmo" ;
                rdfs:comment "Indicates that the controller decides on processing of personal data."@en ;
                rdfs:label "decide sobre"@pt-br ,
                           "decides on"@en ;
                skos:example "The controller is someone who decides on the purposes and means of processing."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#deliversInformationTo
cosmo:deliversInformationTo rdf:type owl:ObjectProperty ,
                                     owl:AsymmetricProperty ,
                                     owl:IrreflexiveProperty ;
                            rdfs:domain cosmo:PrivacyNotice ;
                            rdfs:range cosmo:DigitalSocialMediaUser ;
                            <http://purl.org/dc/elements/1.1/identifier> "159-cosmo" ;
                            rdfs:comment "Indicates that a notice or document communicates information directly to the user."@en ;
                            rdfs:label "delivers information to"@en ,
                                       "entrega informação a"@pt-br ;
                            skos:example "A privacy notice delivers information to the user about their data rights."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#detailsProcessing
cosmo:detailsProcessing rdf:type owl:ObjectProperty ,
                                 owl:AsymmetricProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain cosmo:PrivacyPolicy ;
                        rdfs:range cosmo:Processing ;
                        <http://purl.org/dc/elements/1.1/identifier> "160-cosmo" ;
                        rdfs:comment "Indicates that a document specifies or explains data processing operations."@en ;
                        rdfs:label "details processing"@en ,
                                   "detalha o processamento de"@pt-br ;
                        skos:example "A privacy policy details processing of personal data by the platform."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#establishesStatus
cosmo:establishesStatus rdf:type owl:ObjectProperty ;
                        owl:inverseOf cosmo:isStatusEstablishedBy ;
                        rdf:type owl:AsymmetricProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain cosmo:ConsentDecision ;
                        rdfs:range dpv:ConsentStatus ;
                        <http://purl.org/dc/elements/1.1/identifier> "161-cosmo" ;
                        rdfs:comment "Relates a consent decision act (occurrent) to the legal state (disposition) it establishes."@en ;
                        rdfs:label "estabelece o status"@pt-br ,
                                   "establishes status"@en ;
                        skos:example "An act of granting consent establishes a status of 'given'."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#exploits
cosmo:exploits rdf:type owl:ObjectProperty ;
               owl:inverseOf cosmo:isExploitedBy ;
               rdf:type owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( cosmo:DarkPatternAction
                                           cosmo:DarkPatternArtifact
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( cosmo:CognitiveBias
                                          cosmo:LimitationOfCognitiveCapacity
                                        )
                          ] ;
               <http://purl.org/dc/elements/1.1/identifier> "162-cosmo" ;
               rdfs:comment "Indicates that a dark pattern leverages a specific cognitive bias or limitation to be effective."@en ;
               rdfs:label "exploits"@en ,
                          "explora"@pt-br ;
               skos:example "An Overloading pattern exploits the user's limited attention."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#expressesPreferenceFor
cosmo:expressesPreferenceFor rdf:type owl:ObjectProperty ,
                                      owl:AsymmetricProperty ,
                                      owl:IrreflexiveProperty ;
                             rdfs:domain cosmo:DigitalSocialMediaUser ;
                             rdfs:range cosmo:PrivacyPreference ;
                             <http://purl.org/dc/elements/1.1/identifier> "163-cosmo" ;
                             rdfs:comment "Indicates that a user expresses specific privacy preferences regarding data processing."@en ;
                             rdfs:label "expressa preferência por"@pt-br ,
                                        "expresses preference for"@en ;
                             skos:example "The user expresses preference for their privacy preference to limit data sharing."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#governsUsageOf
cosmo:governsUsageOf rdf:type owl:ObjectProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain cosmo:TermsAndConditions ;
                     rdfs:range cosmo:OnlinePlatform ;
                     <http://purl.org/dc/elements/1.1/identifier> "164-cosmo" ;
                     rdfs:comment "Indicates that a document regulates how a user can interact with a platform or service."@en ;
                     rdfs:label "governs usage of"@en ,
                                "regula o uso de"@pt-br ;
                     skos:example "Terms and Conditions governs the usage of a digital social media platform."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasAccount
cosmo:hasAccount rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain cosmo:DigitalSocialMediaUser ;
                 rdfs:range cosmo:DigitalSocialMediaAccount ;
                 <http://purl.org/dc/elements/1.1/identifier> "165-cosmo" ;
                 rdfs:comment "Indicates that a user has an account on a platform."@en ;
                 rdfs:label "has account"@en ,
                            "tem conta"@pt-br ;
                 skos:example "A user has account on a platform."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasAppointee
cosmo:hasAppointee rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain cosmo:AppointingProcess ;
                   rdfs:range cosmo:DataProtectionOfficer ;
                   <http://purl.org/dc/elements/1.1/identifier> "166-cosmo" ;
                   rdfs:comment "The DPO is appointed by the data controller and the data processor"@en ;
                   rdfs:isDefinedBy "BRASIL. Lei nº 13.709, de 14 de agosto de 2018. Lei Geral de Proteção de Dados Pessoais (LGPD). Brasília, 2018. Available at: https://www.in.gov.br/en/web/dou/-/lei-n-13.709-de-14-de-agosto-de-2018-174008209."@en ;
                   rdfs:label "has appointee"@en ,
                              "tem nomeado"@pt-br ;
                   skos:example "The Data Protection Officer has been appointed by the Controller."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasAppointer
cosmo:hasAppointer rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain cosmo:AppointingProcess ;
                   rdfs:range cosmo:Controller ;
                   <http://purl.org/dc/elements/1.1/identifier> "167-cosmo" ;
                   rdfs:comment "Indicates that a controller appoints a Data Protection Officer."@en ;
                   rdfs:label "has appointer"@en ,
                              "tem nomeante"@pt-br ;
                   skos:example "A Controller appoints a Data Protection Officer."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasCognitiveCapacity
cosmo:hasCognitiveCapacity rdf:type owl:ObjectProperty ,
                                    owl:AsymmetricProperty ,
                                    owl:IrreflexiveProperty ;
                           rdfs:domain cosmo:NaturalPerson ;
                           rdfs:range cosmo:CognitiveCapacity ;
                           <http://purl.org/dc/elements/1.1/identifier> "168-cosmo" ;
                           rdfs:label "Relates an agent to their cognitive capacity."@en ,
                                      "has cognitive capacity"@en ,
                                      "tem capacidade cognitiva"@pt-br ;
                           skos:example "Every natural person has cognitive capacity."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasConsentDecisionBy
cosmo:hasConsentDecisionBy rdf:type owl:ObjectProperty ,
                                    owl:AsymmetricProperty ,
                                    owl:IrreflexiveProperty ;
                           rdfs:domain dpv:ConsentStatus ;
                           rdfs:range cosmo:DataSubject ;
                           <http://purl.org/dc/elements/1.1/identifier> "169-cosmo" ;
                           rdfs:comment "Relates a consent decision to the agent who makes it."@en ;
                           rdfs:label "decisão de consentimento tomada por"@pt-br ,
                                      "has consent decision by"@en ;
                           skos:example "The consent decision is made by the data subject."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasDocument
cosmo:hasDocument rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain cosmo:OnlinePlatform ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( cosmo:PolicyDocument
                                             cosmo:TermsAndConditions
                                           )
                             ] ;
                  <http://purl.org/dc/elements/1.1/identifier> "170-cosmo" ;
                  rdfs:comment "Indicates that a platform provides or makes available a document."@en ;
                  rdfs:label "has document"@en ,
                             "tem documento"@pt-br ;
                  skos:example "An online platform has a privacy policy document for its users."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasFormalAct
cosmo:hasFormalAct rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain cosmo:AppointingProcess ;
                   rdfs:range cosmo:FormalActOfAppointment ;
                   <http://purl.org/dc/elements/1.1/identifier> "171-cosmo" ;
                   rdfs:comment "A property that links an appointing process to the formal act or document that officially records and gives legal effect to that appointment."@en ;
                   rdfs:label "has formal act"@en ,
                              "tem ato formal"@pt-br ;
                   skos:example "The appointing process has formal act that confirms the appointment."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasPersonalData
cosmo:hasPersonalData rdf:type owl:ObjectProperty ,
                               owl:InverseFunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain cosmo:NaturalPerson ;
                      rdfs:range cosmo:PersonalData ;
                      <http://purl.org/dc/elements/1.1/identifier> "172-cosmo" ;
                      rdfs:comment "Indicates that a natural person has personal data."@en ,
                                   "tem dados pessoais"@pt-br ;
                      rdfs:label "has personal data"@en ;
                      skos:example "A data subject has personal data, such as their name or email."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasQuality
cosmo:hasQuality rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 <http://purl.org/dc/elements/1.1/identifier> "173-cosmo" ;
                 rdfs:comment "A relation that links a class to a specific qualifying attribute it must possess."@en ;
                 rdfs:label "has quality"@en ,
                            "tem qualidade"@pt-br ;
                 skos:example "A valid consent has quality of being informed."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasRecipient
cosmo:hasRecipient rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain cosmo:Processing ;
                   rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                   <http://purl.org/dc/elements/1.1/identifier> "174-cosmo" ;
                   rdfs:comment "A property that links a processing activity to the agent who receives the data."@en ;
                   rdfs:label "has recipient"@en ,
                              "tem destinatário"@pt-br ;
                   skos:example "A data transfer operation has a Third Party as its recipient."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasRight
cosmo:hasRight rdf:type owl:ObjectProperty ;
               owl:inverseOf cosmo:isRightOf ;
               rdf:type owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain cosmo:NaturalPerson ;
               rdfs:range cosmo:Right ;
               <http://purl.org/dc/elements/1.1/identifier> "175-cosmo" ;
               rdfs:comment "A relation that links a data subject to a right they possess."@en ;
               rdfs:label "has right"@en ,
                          "tem direito"@pt-br ;
               skos:example "A Data Subject has the right of erasure."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasRole
cosmo:hasRole rdf:type owl:ObjectProperty ;
              owl:inverseOf cosmo:isRoleOf ;
              rdf:type owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              <http://purl.org/dc/elements/1.1/identifier> "176-cosmo" ;
              rdfs:comment "Indicates that an agent assumes a specific role in the context of data processing."@en ;
              rdfs:label "has role"@en ,
                         "tem papel"@pt-br ;
              skos:example "A Data Processing Agent has a role of Controller."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasUser
cosmo:hasUser rdf:type owl:ObjectProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain cosmo:OnlinePlatform ;
              rdfs:range cosmo:DigitalSocialMediaUser ;
              <http://purl.org/dc/elements/1.1/identifier> "177-cosmo" ;
              rdfs:comment "Indicates that digital social media has users."@en ;
              rdfs:label "has user"@en ,
                         "tem usuário"@pt-br ;
              skos:example "An online platform has users."@en .


###  http://www.semanticweb.org/cherr/ontologies/2025/2/cosmo#hasVitiatingFactor
cosmo:hasVitiatingFactor rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         owl:inverseOf cosmo:isVitiatingFactorOf ;
                         rdf:type owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
