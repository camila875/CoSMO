cosmo:ConsentGiven rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty cosmo:hasQuality ;
                                                                owl:someValuesFrom cosmo:FreeConsent
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty cosmo:hasQuality ;
                                                                owl:someValuesFrom cosmo:InformedConsent
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty cosmo:hasQuality ;
                                                                owl:someValuesFrom cosmo:UnequivocalConsent
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty cosmo:isGrantedBy ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass cosmo:DataSubject
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf gc:StatusValidForProcessing ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty cosmo:isStatusEstablishedBy ;
                                     owl:someValuesFrom cosmo:ConsentGranting
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty dpv:hasStatus ;
